{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "vanilla_ffgt",
    "dataset": "PATTERN_pq_012_001_005",
    
    "out_dir": "out/SBM-PATTERN/pq_012_001_005/vanilla/",
    
    "params": {
        "seed": 42,
        "num_workers": 8,
